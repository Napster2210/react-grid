{
  "name": "react-grid",
  "version": "1.0.0",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "client": "cd ../client && npm run start",
    "server": "tsc && concurrently \"tsc -w\" \"nodemon dist/app.js\"",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-validation": "^3.0.2",
    "helmet": "^3.22.0",
    "http-errors": "~1.6.3",
    "mongoose": "^5.9.13",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.46",
    "@types/node": "^13.13.5",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  }
}